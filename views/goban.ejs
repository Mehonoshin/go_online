<html>
  <head>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
    <script src="/gobans.js"></script>
    <script src="/goGame.js"></script>
    <script src="/game.js"></script>
    <script src="/view.js"></script>
    <script src="/controller.js"></script>
    <script src="http://localhost:8889/socket.io/socket.io.js"></script>
    <script>
      $(document).ready(function() {
        gameController = new Controller($('#canvas'), $('#goban'), $('#goban').data('gobanSize'));
      });
    </script>

    <style type="text/css" media="screen">
      canvas {
        cursor: crosshair;
      }
    </style>
  </head>

  <body>
  <div id="goban" data-user-id="<%= userId %>" data-game-id="<%= gameId %>" data-color="<%= color %>" data-goban-size="<%= gobanSize %>">
      <div id="flash"></div>
      <canvas id="canvas"></canvas>
    </div>

  </body>
</html>
